{
  "name": "nuxtlify",
  "version": "1.0.0",
  "description": "Blog template for Nuxt.js/Netlify",
  "author": "Daniel Kelly",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "precommit": "npm run lint",
    "before-build": "node -r esm ./build-tools/beforeBuild.js",
    "after-build": "node -r esm ./build-tools/afterBuild.js",
    "dev": "npm run before-build && nuxt -o && npm run after-build",
    "build": "npm run before-build && nuxt build && npm run after-build",
    "start": "nuxt start",
    "generate": "npm run before-build && nuxt generate && npm run after-build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.7",
    "@fortawesome/free-brands-svg-icons": "^5.4.2",
    "@fortawesome/free-regular-svg-icons": "^5.4.2",
    "@fortawesome/free-solid-svg-icons": "^5.4.2",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@nuxtjs/axios": "^5.9.0",
    "@nuxtjs/google-analytics": "^2.2.2",
    "animate.css": "^3.7.2",
    "axios": "^0.19.0",
    "esm": "^3.2.25",
    "gray-matter": "^4.0.2",
    "highlight.js": "^9.17.1",
    "html-loader": "^0.5.5",
    "intersection-observer": "^0.7.0",
    "lodash": "^4.17.15",
    "markdown-it": "^8.4.2",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-video": "^0.6.3",
    "md5": "^2.2.1",
    "ncp": "^2.0.0",
    "netlify-cms": "^2.10.2",
    "nuxt": "^2.11.0",
    "nuxt-buefy": "^0.3.19",
    "nuxt-fontawesome": "^0.4.0",
    "opti-image": "^0.10.0",
    "react": "^16.12.0",
    "showdown": "^1.9.1",
    "url-parse": "^1.4.7",
    "v-runtime-template": "^1.10.0",
    "vue": "^2.6.11",
    "vue-disqus": "^3.0.5"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/eslint-module": "^0.0.1",
    "@nuxtjs/style-resources": "^0.1.2",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-nuxt": "^0.5.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "gray-matter-loader": "0.0.10",
    "node-sass": "^4.13.0",
    "nodemon": "^1.18.9",
    "nuxt-responsive-loader": "^1.0.6",
    "prettier": "^1.19.1",
    "rimraf": "^2.7.1",
    "sass-loader": "^7.3.1",
    "sharp": "^0.23.2"
  }
}
